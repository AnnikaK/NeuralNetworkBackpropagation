import java.util.Arrays;

import javafx.application.Platform;

public class Fighter {

	private int[] nothing;

	private int[]
	

	public Fighter() {

	}

	public void showReaction(int[] outputVector) {

	}

	private boolean equal(int[] a, int[] b) {
		if (a.length != b.length) {
			return false;
		}
		for (int i = 0; i < a.length; i++) {
			if (Math.abs(a[i] - b[i]) > 0.1) {   //toleranzbereich weil 1 nie erreicht
				return false;
			}
		}
		return true;
	}

}
